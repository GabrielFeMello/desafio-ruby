<p id="notice"><%= notice %></p>

<h1>Transations</h1>

<%= form_tag  ('/transations/cnab_upload') ,multipart: true do %>
  <label for="file">Envie um arquivo CNAB de padrão Comum</label> <%= file_field_tag "file" %>
  <div><%= submit_tag 'Clique para processá-lo' %></div>
<% end %>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Date</th>
      <th>Value</th>
      <th>Representative</th>
      <th>Store</th>
      <th>Card</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transations.each do |transation| %>
      <tr>
        <td><%= transation.type %></td>
        <td><%= transation.date %></td>
        <td><%= transation.value %></td>
        <td><%= transation.representative %></td>
        <td><%= transation.store %></td>
        <td><%= transation.card %></td>
        <td><%= transation.time %></td>
        <td><%= link_to 'Show', transation %></td>
        <td><%= link_to 'Edit', edit_transation_path(transation) %></td>
        <td><%= link_to 'Destroy', transation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transation', new_transation_path %>
